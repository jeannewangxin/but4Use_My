<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.but4reuse.adapters">
      <adapter
            class="org.but4reuse.adapters.graphs.GraphsAdapter"
            id="graphs"
            name="Graphs">
         <elements>
            <element
                  element="org.but4reuse.adapters.graphs.EdgeElement"></element>
            <element
                  element="org.but4reuse.adapters.graphs.VertexElement">
            </element>
         </elements>
      </adapter>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.but4reuse.adapters.ui.preferences.AdaptersPreferencePage"
            class="org.but4reuse.adapters.graphs.preferences.GraphsAdapterPreferencePage"
            id="org.but4reuse.adapters.graphs.page"
            name="Graphs">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.but4reuse.adapters.graphs.preferences.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
