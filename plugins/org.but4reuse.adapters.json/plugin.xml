<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.but4reuse.adapters">
      <adapter
            class="org.but4reuse.adapters.json.JsonAdapter"
            icon="icons/json_icon.png"
            id="json"
            name="json">
         <elements>
            <element
                  element="org.but4reuse.adapters.json.KeyElement"></element>
            <element
                  element="org.but4reuse.adapters.json.ValueElement">
            </element>
            <element
                  element="org.but4reuse.adapters.json.ObjectElement">
            </element>
            <element
                  element="org.but4reuse.adapters.json.ArrayElement">
            </element>
            <element
                  element="org.but4reuse.adapters.json.IndexArrayElement">
            </element>
            <element
                  element="org.but4reuse.adapters.json.IgnoredElement">
            </element>
            <element
                  element="org.but4reuse.adapters.json.UnsplittableElement">
            </element>
         </elements>
      </adapter>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.but4reuse.adapters.ui.preferences.AdaptersPreferencePage"
            class="org.but4reuse.adapters.json.preferences.JsonAdapterPreferencePage"
            id="org.but4reuse.adapters.json.page"
            name="Json">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.but4reuse.adapters.json.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
